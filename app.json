{
  "expo": {
    "name": "AeroMobile",
    "slug": "AeroMobile",
    "description": "A Mobile App that Manages your AeroHouse System",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/logo/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/images/splash1.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/logo/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
